import kotlin.Boolean;

CREATE TABLE IF NOT EXISTS CollectionItemType(
    code TEXT PRIMARY KEY,
    name TEXT NOT NULL
);

INSERT INTO CollectionItemType VALUES
    ("hieroglyph", "Hieroglyph"),
    ("empty", "Empty");

CREATE TABLE IF NOT EXISTS CollectionItemVariant(
    id TEXT PRIMARY KEY,
    collectionId TEXT NOT NULL,
    name TEXT NOT NULL,
    type INTEGER -- 0 - any, 1 - primary, 2 - secondary
);

INSERT INTO CollectionItemVariant(id, collectionId, name, type) VALUES
    ("47dab697-3ace-42df-9be3-181543845b56", "38ddff20-2211-4073-a5f8-b13cc572e473", "Hiragana", 1),
    ("c524193e-4d77-49e4-89ad-05577234a271", "38ddff20-2211-4073-a5f8-b13cc572e473", "Romaji", 2),
    ("cfb78984-3fc1-4af7-be8c-52053989a0e4", "b4741f39-9fac-4189-b8d7-24b4931db9b3", "Katakana", 1),
    ("979dd3ae-0b1b-4c51-913d-d5e85461309a", "b4741f39-9fac-4189-b8d7-24b4931db9b3", "Romaji", 2)
;

CREATE TABLE IF NOT EXISTS CollectionItem (
    id TEXT PRIMARY KEY,
    sectionId TEXT NOT NULL REFERENCES Section(id),
    type TEXT NOT NULL REFERENCES CollectionItemType(code),
    sort INTEGER NOT NULL,
    isSelected INTEGER AS Boolean NOT NULL DEFAULT 0,
    isBookmarked INTEGER AS Boolean NOT NULL DEFAULT 0
);

CREATE TABLE IF NOT EXISTS CollectionItemVariantItem(
    collectionItemId TEXT NOT NULL,
    collectionItemVariant TEXT NOT NULL,
    content TEXT NOT NULL
);

